						<button class="btn btn-info" (click)="${action.triggerName}(#renderAngularTableCommandLinkParameters() )">
							$stringUtils.capitalize($action.trigger.messageKey)
						</button>
					</div>
				</td>
#end
#foreach ($column in $table.tableColumns)
				<td>{{ row.${column.name} }}</td>
#end
			</tr>
		</tbody>
	</table>
#end
</div>
