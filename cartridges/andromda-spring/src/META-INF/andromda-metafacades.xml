<?xml version="1.0" encoding="ISO-8859-1" ?>

<!-- contains the hibernate cartridge metafacade mappings -->
<metafacades namespace="spring">
    <metafacade class="org.andromda.cartridges.spring.metafacades.HibernateFinderMethodLogicImpl">
        <mapping class="org.omg.uml.foundation.core.Operation$Impl">
            <stereotype>FINDER_METHOD</stereotype>            
        </mapping>
        <property reference="hibernateQueryUseNamedParameters" default="false"/>
        <property reference="hibernateUseQueryCache" default="false"/>
    </metafacade>
    <metafacade class="org.andromda.cartridges.spring.metafacades.HibernateTypeLogicImpl">
        <mapping class="org.omg.uml.foundation.core.DataType$Impl"/>
        <property reference="hibernateTypeMappingsUri" default=""/>
    </metafacade>
    <metafacade class="org.andromda.cartridges.spring.metafacades.SpringServiceLogicImpl" contextRoot="true">
        <mapping class="org.omg.uml.foundation.core.UmlClass$Impl">
            <stereotype>SERVICE</stereotype>          
        </mapping>        
        <property reference="ejbJndiNamePrefix" default=""/>
        <property reference="ejbPackageNamePattern" default="{0}.ejb"/>
        <property reference="defaultServiceExceptionNamePattern" default="{0}Exception"/>
        <property reference="defaultServiceExceptions" default="true"/>
    </metafacade>
    <metafacade class="org.andromda.cartridges.spring.metafacades.SpringServiceLogicImpl" contextRoot="true">
        <mapping class="org.omg.uml.foundation.core.Interface$Impl">
            <stereotype>SERVICE</stereotype>          
        </mapping>       
        <property reference="ejbJndiNamePrefix" default=""/>
        <property reference="ejbPackageNamePattern" default="{0}.ejb"/>
        <property reference="defaultServiceExceptionNamePattern" default="{0}Exception"/>
        <property reference="defaultServiceExceptions" default="true"/>
    </metafacade>
    <metafacade class="org.andromda.cartridges.spring.metafacades.SpringEntityLogicImpl" contextRoot="true">
        <mapping class="org.omg.uml.foundation.core.UmlClass$Impl">
            <stereotype>ENTITY</stereotype>         
        </mapping>   
        <property reference="entityNamePattern" default="{0}"/>
        <property reference="defaultHibernateGeneratorClass" default="native"/>
        <property reference="hibernateDefaultCascade" default="none"/>
        <property reference="hibernateEntityCache" default=""/>
    </metafacade>
    <metafacade class="org.andromda.cartridges.spring.metafacades.SpringEntityLogicImpl" contextRoot="true">
        <mapping class="org.omg.uml.foundation.core.Classifier$Impl">
            <stereotype>ENTITY</stereotype>         
        </mapping>   
        <property reference="entityNamePattern" default="{0}"/>
        <property reference="defaultHibernateGeneratorClass" default="native"/>
        <property reference="hibernateDefaultCascade" default="none"/>
        <property reference="hibernateEntityCache" default=""/>
    </metafacade>
    <metafacade class="org.andromda.cartridges.spring.metafacades.SpringEntityLogicImpl" contextRoot="true">
        <mapping class="org.omg.uml.foundation.core.Interface$Impl">
            <stereotype>ENTITY</stereotype>         
        </mapping>  
        <stereotype>ENTITY</stereotype>
        <property reference="entityNamePattern" default="{0}"/>
        <property reference="defaultHibernateGeneratorClass" default="native"/>
        <property reference="hibernateDefaultCascade" default="none"/>
        <property reference="hibernateEntityCache" default=""/>
    </metafacade>
    <metafacade class="org.andromda.cartridges.spring.metafacades.SpringEntityAssociationEndLogicImpl">
        <mapping class="org.omg.uml.foundation.core.AssociationEnd$Impl">
            <context>org.andromda.cartridges.spring.metafacades.SpringEntity</context>       
        </mapping>  
        <property reference="hibernateAssociationEndOuterJoin" default="auto"/>
    </metafacade>
    <metafacade class="org.andromda.cartridges.spring.metafacades.SpringEntityAssociationLogicImpl">
        <mapping class="org.omg.uml.foundation.core.UmlAssociation$Impl">
            <context>org.andromda.cartridges.spring.metafacades.SpringEntity</context>       
        </mapping>  
    </metafacade>
    <metafacade class="org.andromda.cartridges.spring.metafacades.SpringServiceOperationLogicImpl">
        <mapping class="org.omg.uml.foundation.core.Operation$Impl">
            <context>org.andromda.cartridges.spring.metafacades.SpringService</context> 
        </mapping> 
        <property reference="implementationOperationNamePrefix" default="handle"/>
    </metafacade>
    <metafacade class="org.andromda.cartridges.spring.metafacades.SpringEntityOperationLogicImpl">
        <mapping class="org.omg.uml.foundation.core.Operation$Impl">
            <context>org.andromda.cartridges.spring.metafacades.SpringEntity</context>       
        </mapping>  
        <property reference="implementationOperationNamePrefix" default="handle"/>
    </metafacade>
    <metafacade class="org.andromda.cartridges.spring.metafacades.SpringDependencyLogicImpl">
        <mapping class="org.omg.uml.foundation.core.Dependency$Impl"/>
    </metafacade>
    <metafacade class="org.andromda.cartridges.spring.metafacades.SpringEnumerationLogicImpl">
        <mapping class="org.omg.uml.foundation.core.Classifier$Impl">
            <stereotype>ENUMERATION</stereotype>    
        </mapping>  
    </metafacade>
    <metafacade class="org.andromda.cartridges.spring.metafacades.SpringEnumerationLogicImpl">
        <mapping class="org.omg.uml.foundation.core.Interface$Impl">
            <stereotype>ENUMERATION</stereotype>    
        </mapping>  
    </metafacade>
    <metafacade class="org.andromda.cartridges.spring.metafacades.SpringEnumerationLogicImpl">
        <mapping class="org.omg.uml.foundation.core.UmlClass$Impl">
            <stereotype>ENUMERATION</stereotype>    
        </mapping>  
    </metafacade>
</metafacades>