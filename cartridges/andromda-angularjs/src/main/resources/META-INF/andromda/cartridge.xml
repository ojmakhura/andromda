		<!-- Creates only one JS file that contains informations about all AngularJS Factories elements -->
	<template
		path="templates/angularjs/services/Services.vsl"
		outputPattern="components/services.js"
		outlet="controllers"
		overwrite="true"
		outputToSingleFile="true"
		outputOnEmptyElements="false">
		<modelElements variable="angularFactoryFacades">
			<modelElement>
				<type name="org.andromda.cartridges.angularjs.metafacades.AngularFactory" />
			</modelElement>
		</modelElements>
	</template>
	  
   <template
        path="templates/angularjs/App.js.vsl"
        outputPattern="app.js"
        outlet="controllers"
        outputToSingleFile="true"
        outputOnEmptyElements="false"
        overwrite="true">
        <modelElements variable="controllers">
            <modelElement>
                <type name="org.andromda.metafacades.uml.FrontEndController">
                    <property name="useCase"/>
                </type>
            </modelElement>
            <modelElement>
				<type name="org.andromda.cartridges.angularjs.metafacades.AngularFactory" />
			</modelElement>
        </modelElements>
    </template>
    <template
        path="templates/angularjs/controllers/Controllers.vsl"
        outputPattern="components/controllers.js"
        outlet="controllers"
        outputToSingleFile="true"
        outputOnEmptyElements="false"
        overwrite="true">
        <modelElements variable="controllers">
            <modelElement>
                <type name="org.andromda.cartridges.angularjs.metafacades.AngularController">
                    <property name="useCase"/>
                </type>
            </modelElement>
            <modelElement>
				<type name="org.andromda.cartridges.angularjs.metafacades.AngularAction" >
				<property name="useCase"/>
				</type>
			</modelElement>
   </modelElements>
    </template>

    <template
        path="templates/angularjs/views/View.vsl"
        outputPattern="components/{1}/{1}.html"
        outlet="controllers"
        outputToSingleFile="false"
        overwrite="true">
        <modelElements variable="viewer">
            <modelElement>
                <type name="org.andromda.cartridges.angularjs.metafacades.AngularView">
                    <property name="useCase"/>
                </type>
            </modelElement>
        </modelElements>
    </template>

    <template
        path="templates/angularjs/views/HomeScreen.vsl"
        outputPattern="index.html"
        outlet="controllers"
        outputToSingleFile="true"
        overwrite="true">
       <modelElements variable="pages">
            <modelElement>
                <type name="org.andromda.cartridges.angularjs.metafacades.AngularController">
                    <property name="useCase"/>
                </type>
            </modelElement>
            <modelElement>
				<type name="org.andromda.cartridges.angularjs.metafacades.AngularFactory" />
			</modelElement>
        </modelElements>
    </template>
    <template
        path="templates/angularjs/views/Header.vsl"
        outputPattern="components/Header.html"
        outlet="controllers"
        outputToSingleFile="true"
        overwrite="true">
       <modelElements variable="pages">
            <modelElement>
                <type name="org.andromda.cartridges.angularjs.metafacades.AngularController">
                    <property name="useCase"/>
                </type>
            </modelElement>
        </modelElements>
    </template>
    
    <template
        path="templates/angularjs/views/Content.vsl"
        outputPattern="components/Content.html"
        outlet="controllers"
        outputToSingleFile="true"
        overwrite="true">
       <modelElements variable="pages">
            <modelElement>
                <type name="org.andromda.metafacades.uml.FrontEndController">
                    <property name="useCase"/>
                </type>
            </modelElement>
            <modelElement>
				<type name="org.andromda.cartridges.angularjs.metafacades.AngularFactory" />
			</modelElement>
        </modelElements>
    </template>
