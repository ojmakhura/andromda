##
##    This JSP fragment stores all hidden parameters for an action.
##
#foreach ($field in $action.hiddenParameters)
<h:inputHidden id="${field.name}" value="#{${action.formBeanName}.${field.name}}">
#if ($field.type.dateType)
    <jsf:convertDateTime pattern="$defaultDateFormat"/>
#end
</h:inputHidden>
#end 