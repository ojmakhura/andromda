<project>
    <pomVersion>3</pomVersion>
    <artifactId>andromda-contrib</artifactId>
    <name>AndroMDA Contributions</name>
    <groupId>andromda</groupId>
    <organization/>
    <description>This module stores all those contributions to AndroMDA that have not yet been moved to the AndroMDA distribution (but could at some point).</description>
    <shortDescription>AndroMDA Contributions</shortDescription>
    <repository>
        <connection>scm:cvs:${repository.connection.mode}:${repository.connection.user}@andromda.cvs.sourceforge.net:/cvsroot/andromda:contrib</connection>
        <developerConnection>scm:cvs:ext:${maven.username}@andromda.cvs.sourceforge.net:/cvsroot/andromda:contrib</developerConnection>
        <url>http://andromda.cvs.sourceforge.net/andromda/contrib</url>
    </repository>
    <dependencies>
        <dependency>
            <groupId>andromda</groupId>
            <artifactId>andromda-metafacades-uml</artifactId>
            <version>${andromda.version}</version>
            <type>xml.zip</type>
        </dependency>
        <dependency>
            <groupId>andromda</groupId>
            <artifactId>andromda-profile</artifactId>
            <version>${andromda.version}</version>
            <type>xml.zip</type>
        </dependency>
        <dependency>
            <groupId>andromda</groupId>
            <artifactId>andromda-profile-datatype</artifactId>
            <version>${andromda.version}</version>
            <type>xml.zip</type>
        </dependency>
        <dependency>
            <groupId>andromda</groupId>
            <artifactId>andromda-profile-webservice</artifactId>
            <version>${andromda.version}</version>
            <type>xml.zip</type>
        </dependency>
        <dependency>
            <groupId>andromda</groupId>
            <artifactId>andromda-profile-service</artifactId>
            <version>${andromda.version}</version>
            <type>xml.zip</type>
        </dependency>
        <dependency>
            <groupId>andromda</groupId>
            <artifactId>andromda-profile-process</artifactId>
            <version>${andromda.version}</version>
            <type>xml.zip</type>
        </dependency>
        <dependency>
            <groupId>andromda</groupId>
            <artifactId>andromda-profile-meta</artifactId>
            <version>${andromda.version}</version>
            <type>xml.zip</type>
        </dependency>
        <dependency>
            <groupId>andromda</groupId>
            <artifactId>andromda-profile-xml</artifactId>
            <version>${andromda.version}</version>
            <type>xml.zip</type>
        </dependency>
        <dependency>
            <groupId>andromda</groupId>
            <artifactId>andromda-profile-persistence</artifactId>
            <version>${andromda.version}</version>
            <type>xml.zip</type>
        </dependency>
        <dependency>
            <groupId>andromda</groupId>
            <artifactId>andromda-profile-presentation</artifactId>
            <version>${andromda.version}</version>
            <type>xml.zip</type>
        </dependency>
    </dependencies>
    <build>
        <sourceDirectory>src/java</sourceDirectory>
        <unitTestSourceDirectory>src/test</unitTestSourceDirectory>
        <unitTest>
            <includes>
                <include>**/Test*.java</include>
            </includes>
        </unitTest>
        <resources>
            <resource>
                <directory>src/java</directory>
                <excludes>
                    <exclude>**/*.java</exclude>
                </excludes>
                <filtering>false</filtering>
            </resource>
            <resource>
                <directory>target/src/java</directory>
                <excludes>
                    <exclude>**/*.java</exclude>
                </excludes>
                <filtering>false</filtering>
            </resource>
            <resource>
                <directory>src</directory>
                <excludes>
                    <exclude>java/**/*.java</exclude>
                    <exclude>uml/**/*</exclude>
                    <exclude>test/**/*</exclude>
                </excludes>
                <filtering>false</filtering>
            </resource>
        </resources>
    </build>
    <reports>
        <report>maven-jxr-plugin</report>
        <report>maven-javadoc-plugin</report>
        <report>maven-simian-plugin</report>
        <report>maven-pmd-plugin</report>
        <report>maven-jdepend-plugin</report>
    </reports>
    <properties/>
</project>