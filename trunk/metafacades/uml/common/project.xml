<?xml version="1.0" encoding="UTF-8"?>

<project>
  	<extend>../project.xml</extend>
  	<artifactId>andromda-metafacades-uml</artifactId>
  	<name>UML Common Metafacades</name>
  	<shortDescription>UML Common  Metafacade Library</shortDescription>
  	<description>
  		The common UML metafacades.  This is the set of core UML metafacades
  		used to provide access UML meta models. 
  	</description>
  	<repository>
    	<connection>scm:cvs:pserver:anonymous@cvs.sourceforge.net:/cvsroot:andromda/metafacades/uml/common</connection>
    	<url>http://cvs.sourceforge.net/viewcvs.py/andromda/metafacades/uml/common</url>
  	</repository>
	<dependencies>
    	<dependency>
	      	<groupId>andromda-bootstrap</groupId>
	      	<artifactId>maven-andromda-bootstrap-plugin</artifactId>
	      	<version>${pom.currentVersion}</version>
	      	<type>plugin</type>
	      	<properties>
	      		<languageMappingsUri>Java</languageMappingsUri>
                <wrapperMappingsUri>JavaWrapper</wrapperMappingsUri>
	      		<facades>${basedir}/target/src</facades>
	      		<facade-logics.ignore/>
	      		<facades-to-impls.ignore/>
	      		<facade-logic-impls.ignore/>
	      	</properties>
	    </dependency>
    </dependencies>
</project>
