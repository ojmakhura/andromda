<?xml version="1.0" encoding="ISO-8859-1" ?>
<!--
  The base UML Profile
-->
<mappings name="uml">
    <!-- stereotypes -->
    <mapping>
        <from>CRITERIA</from>
        <to>Criteria</to>
    </mapping>
    <mapping>
        <from>ENTITY</from>
        <to>Entity</to>
    </mapping>
    <mapping>
        <from>SERVICE</from>
        <to>Service</to>
    </mapping>
    <mapping>
        <from>FINDER_METHOD</from>
        <to>FinderMethod</to>
    </mapping>
    <mapping>
        <from>IDENTIFIER</from>
        <to>Identifier</to>
    </mapping>
    <mapping>
        <from>UNIQUE</from>
        <to>Unique</to>
    </mapping>
    <mapping>
        <from>VALUE_OBJECT</from>
        <to>ValueObject</to>
    </mapping>
    <mapping>
        <from>EXCEPTION</from>
        <to>Exception</to>
    </mapping>
    <mapping>
        <from>APPLICATION_EXCEPTION</from>
        <to>ApplicationException</to>
    </mapping>
    <mapping>
        <from>UNEXPECTED_EXCEPTION</from>
        <to>UnexpectedException</to>
    </mapping>
    <mapping>
        <from>ENUMERATION</from>
        <to>Enumeration</to>
    </mapping>
    <mapping>
        <from>WEBSERVICE</from>
        <to>WebService</to>
    </mapping>
    <mapping>
        <from>WEBSERVICE_OPERATION</from>
        <to>WebServiceOperation</to>
    </mapping>
    <mapping>
        <from>NULLABLE</from>
        <to>Nullable</to>
    </mapping>
    <mapping>
        <from>MANAGEABLE</from>
        <to>Manageable</to>
    </mapping>
    <mapping>
        <from>UNSECURED</from>
        <to>Unsecured</to>
    </mapping>
    <mapping>
        <from>EMBEDDED_VALUE</from>
        <to>EmbeddedValue</to> 
    </mapping>
    <!-- tagged values -->
    <mapping>
        <from>DOCUMENTATION</from>
        <to>documentation</to>
    </mapping>
    <mapping>
        <from>PERSISTENCE_TABLE</from>
        <to>@andromda.persistence.table</to>
    </mapping>
    <mapping>
        <from>PERSISTENCE_COLUMN</from>
        <to>@andromda.persistence.column</to>
    </mapping>
    <mapping>
        <from>PERSISTENCE_COLUMN_LENGTH</from>
        <to>@andromda.persistence.column.length</to>
    </mapping>
    <mapping>
        <from>PERSISTENCE_COLUMN_INDEX</from>
        <to>@andromda.persistence.column.index</to>
    </mapping>
    <mapping>
        <from>PERSISTENCE_FOREIGN_IDENTIFIER</from>
        <to>@andromda.persistence.foreign.identifier</to>
    </mapping>
    <mapping>
        <from>PERSISTENCE_ASSIGNED_IDENTIFIER</from>
        <to>@andromda.persistence.assigned.identifier</to>
    </mapping>
    <mapping>
        <from>PERSISTENCE_FOREIGN_KEY_CONSTRAINT_NAME</from>
        <to>@andromda.persistence.foreignkey.constraint</to>
    </mapping>
    <mapping>
        <from>PERSISTENCE_IMMUTABLE</from>
        <to>@andromda.persistence.immutable</to>
    </mapping>
    <mapping>
        <from>WEBSERVICE_STYLE</from>
        <to>@andromda.webservice.style</to>
    </mapping>
    <mapping>
        <from>WEBSERVICE_USE</from>
        <to>@andromda.webservice.use</to>
    </mapping>
    <mapping>
        <from>WEBSERVICE_PROVIDER</from>
        <to>@andromda.webservice.provider</to>
    </mapping>
    <mapping>
        <from>ROLE_NAME</from>
        <to>@andromda.role.name</to>
    </mapping>
    <mapping>
        <from>SERIALVERSION_UID</from>
        <to>@andromda.serialVersionUID</to>
    </mapping>
    <!-- datatypes -->
    <mapping>
        <from>COLLECTION_TYPE</from>
        <to>datatype::Collection</to>
    </mapping>
    <mapping>
        <from>LIST_TYPE</from>
        <to>datatype::List</to>
    </mapping>
    <mapping>
        <from>SET_TYPE</from>
        <to>datatype::Set</to>
    </mapping>
    <mapping>
        <from>DATE_TYPE</from>
        <to>datatype::Date</to>
    </mapping>
    <mapping>
        <from>BOOLEAN_TYPE</from>
        <to>datatype::boolean</to>
    </mapping>
    <mapping>
        <from>FILE_TYPE</from>
        <to>datatype::File</to>
    </mapping>
    <mapping>
        <from>BLOB_TYPE</from>
        <to>datatype::Blob</to>
    </mapping>
    <mapping>
        <from>MAP_TYPE</from>
        <to>datatype::Map</to>
    </mapping>
    <mapping>
        <from>STRING_TYPE</from>
        <to>datatype::String</to>
    </mapping>
</mappings>