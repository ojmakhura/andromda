<?xml version="1.0" encoding="UTF-8"?>

<project>
  	<extend>../project.xml</extend>
  	<id>andromda-metafacades-uml14</id>
  	<name>UML 1.4 Metafacades</name>
  	<shortDescription>UML 1.4  Metafacade Library</shortDescription>
  	<description>
  		The UML 1.4 metafacades.  This is the set of UML 1.4 metafacades
  		implementations.  These implement the common UML metafacades.
  	</description>
  	<repository>
    	<connection>scm:cvs:pserver:anonymous@cvs.sourceforge.net:/cvsroot:andromda/metafacades/uml/uml14</connection>
    	<url>http://cvs.sourceforge.net/viewcvs.py/andromda/metafacades/uml/uml14</url>
  	</repository>
	<dependencies>
    	<dependency>
	      	<groupId>andromda</groupId>
	      	<artifactId>maven-andromda-plugin-bootstrap</artifactId>
	      	<version>3.0M2-SNAPSHOT</version>
	      	<type>plugin</type>
	      	<properties>
	      		<logical.languageMappingsUri>Java</logical.languageMappingsUri>
	      		<facades.ignore/>
	      		<facade-logics>${basedir}/target/src</facade-logics>
	      		<facades-to-impls>${basedir}/target/src</facades-to-impls>
	      		<facade-logic-impls>${basedir}/src/java</facade-logic-impls>
	      		<metamodelVersionPackage>org.andromda.metafacades.uml14</metamodelVersionPackage>
	      	</properties>
	    </dependency>
    	<dependency>
    	    <groupId>andromda</groupId>
    	    <artifactId>andromda-metafacades-uml</artifactId>
    	    <version>${pom.currentVersion}</version>
    	</dependency>    
        <dependency>
            <groupId>andromda-translation-libraries</groupId>
            <artifactId>andromda-validation-library-bootstrap</artifactId>
            <version>${pom.currentVersion}</version>
        </dependency>
    	<dependency> 
    		<groupId>jmi</groupId>
      		<artifactId>jmiuml</artifactId>
     		<version>1.4di</version>
    	</dependency>   
    	<dependency> 
    		<groupId>jmi</groupId>
      		<artifactId>jmi</artifactId>
     		<version>200401240117</version>
    	</dependency>     
	    <dependency>
	      	<groupId>commons-lang</groupId>
	      	<artifactId>commons-lang</artifactId>
	      	<version>2.0</version>
	    </dependency>	
    	<dependency> 
    		<groupId>log4j</groupId>
      		<artifactId>log4j</artifactId>
     		<version>1.2.8</version>
    	</dependency>	
	</dependencies>	
</project>
