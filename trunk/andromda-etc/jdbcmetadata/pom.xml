                      <contextFile>${project.basedir}/src/test/resources/context.xml</contextFile>
                      <additionalConfigFilesDir>${project.basedir}/src/test/resources</additionalConfigFilesDir>
                    </configuration>
                </plugin>
            </plugins>
        </pluginManagement>
