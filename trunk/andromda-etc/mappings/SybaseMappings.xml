<?xml version="1.0" encoding="UTF-8" ?>
<!--
  This is used to map the model types to
  Sybase types for generated code.
  Information was determined from:
  http://manuals.sybase.com/onlinebooks/group-asarc/srg1100e/sqlref/@Generic__BookView;cs=default;ts=default
-->
<mappings name="Sybase">
    <mapping>
        <from>datatype::String</from>
        <to>VARCHAR(255)</to>
    </mapping>
    <mapping>
        <from>datatype::Date</from>
        <to>DATETIME</to>
    </mapping>
    <mapping>
        <from>datatype::Time</from>
        <to>DATETIME</to>
    </mapping>
    <mapping>
        <from>datatype::Timestamp</from>
        <from>datatype::DateTime</from>
        <to>TIMESTAMP</to>
    </mapping>
    <mapping>
        <from>datatype::char</from>
        <from>datatype::Character</from>
        <to>CHAR</to>
    </mapping>
    <mapping>
        <from>datatype::byte</from>
        <from>datatype::Byte</from>
        <to>TINYINT</to>
    </mapping>
    <mapping>
        <from>datatype::short</from>
        <from>datatype::Short</from>
        <to>SMALLINT</to>
    </mapping>
    <mapping>
        <from>datatype::int</from>
        <from>datatype::Integer</from>
        <to>INTEGER</to>
    </mapping>
    <mapping>
        <from>datatype::long</from>
        <from>datatype::Long</from>
        <to>NUMERIC</to>
    </mapping>
    <mapping>
        <from>datatype::float</from>
        <from>datatype::Float</from>
        <to>REAL</to>
    </mapping>
    <mapping>
        <from>datatype::double</from>
        <from>datatype::Double</from>
        <from>datatype::Money</from>
        <to>DOUBLE PRECISION</to>
    </mapping>
    <mapping>
        <from>datatype::boolean</from>
        <from>datatype::Boolean</from>
        <to>BIT</to>
    </mapping>
    <mapping>
        <from>datatype::Decimal</from>
        <from>datatype::decimal</from>
        <to>NUMERIC</to>
    </mapping>
    <mapping>
        <from>datatype::Clob</from>
        <to>TEXT</to>
    </mapping>
    <mapping>
        <from>datatype::Blob</from>
        <to>IMAGE</to>
    </mapping>
</mappings>