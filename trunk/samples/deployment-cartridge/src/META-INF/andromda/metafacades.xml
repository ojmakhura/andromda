<?xml version="1.0" encoding="ISO-8859-1"?>

<!-- contains the cartridge metafacade mappings -->
<metafacades>
    
    <metafacade class="org.andromda.cartridges.deployment.metafacades.ArtifactFacadeLogicImpl"
        contextRoot="true">
        <mapping class="org.omg.uml.foundation.core.Artifact$Impl" />
    </metafacade>
    
    <metafacade class="org.andromda.cartridges.deployment.metafacades.ComponentFacadeLogicImpl">
        <mapping class="org.omg.uml.foundation.core.Component$Impl" />
    </metafacade>
    
    <metafacade
        class="org.andromda.cartridges.deployment.metafacades.DeployedPackageFacadeLogicImpl">
        <mapping>
            <context>org.andromda.cartridges.deployment.metafacades.ArtifactFacade</context>
        </mapping>
    </metafacade>
    
    <metafacade class="org.andromda.cartridges.deployment.metafacades.NodeFacadeLogicImpl">
        <mapping class="org.omg.uml.foundation.core.Node$Impl" />
    </metafacade>
    
</metafacades>