<?xml version="1.0" encoding="ISO-8859-1" ?>
<!--
  The default Spring profile mappings.
-->
<mappings name="spring">
    <mapping>
        <from>HIBERNATE_QUERY</from>
        <to>@andromda.hibernate.query</to>
    </mapping>
    <mapping>
        <from>HIBERNATE_GENERATOR_CLASS</from>
        <to>@andromda.hibernate.generator.class</to>
    </mapping>
    <mapping>
        <from>HIBERNATE_LAZY</from>
        <to>@andromda.hibernate.lazy</to>
    </mapping>
    <mapping>
        <from>HIBERNATE_INHERITANCE</from>
        <to>@andromda.hibernate.inheritance</to>
    </mapping>
    <mapping>
        <from>HIBERNATE_USE_QUERY_CACHE</from>
        <to>@andromda.hibernate.query.useCache</to>
    </mapping>
    <mapping>
        <from>HIBERNATE_OUTER_JOIN</from>
        <to>@andromda.hibernate.outerjoin</to>
    </mapping>
     <mapping>
        <from>HIBERNATE_ENTITY_CACHE</from>
        <to>@andromda.hibernate.entity.cache</to>
    </mapping>
    <mapping>
        <from>HIBERNATE_ASSOCIATION_CACHE</from>
        <to>@andromda.hibernate.association.cache</to>
    </mapping>
</mappings>