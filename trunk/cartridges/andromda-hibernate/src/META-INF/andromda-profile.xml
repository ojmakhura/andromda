        <to>@andromda.hibernate.ehcache.overflowToDisk</to>
    </mapping>
    <mapping>
        <from>HIBERNATE_GENERATOR_CLASS</from>
        <to>@andromda.hibernate.generator.class</to>
    </mapping>
    <mapping>
        <from>HIBERNATE_PROXY</from>
        <to>@andromda.hibernate.entity.proxy</to>
    </mapping>
    <mapping>
        <from>HIBERNATE_ASSOCIATION_COLLECTION_TYPE</from>
        <to>@andromda.hibernate.collection.type</to>
    </mapping>
    <mapping>
        <from>HIBERNATE_ASSOCIATION_SORT_TYPE</from>
        <to>@andromda.hibernate.sort.type</to>
    </mapping>
    <mapping>
        <from>TAGGEDVALUE_HIBERNATE_ASSOCIATION_ORDER_BY_COLUMNS</from>
        <to>@andromda.hibernate.orderByColumns</to>
    </mapping>
    <mapping>
        <from>HIBERNATE_ASSOCIATION_WHERE_CLAUSE</from>
        <to>@andromda.hibernate.whereClause</to>
    </mapping>
    <mapping>
        <from>HIBERNATE_ASSOCIATION_INDEX</from>
        <to>@andromda.hibernate.collection.index</to>
    </mapping>
    <mapping>
        <from>HIBERNATE_ASSOCIATION_INDEX_TYPE</from>
        <to>@andromda.hibernate.collection.index.type</to>
    </mapping>
    <mapping>
        <from>HIBERNATE_VERSION</from>
        <to>@andromda.hibernate.version</to>
    </mapping>
</mappings>