<?xml version="1.0" encoding="ISO-8859-1" ?>
<!--
  The default Hibernate profile mappings.
-->
<mappings name="hibernate">
    <!-- tagged values -->
    <mapping>
        <from>HIBERNATE_QUERY</from>
        <to>@andromda.hibernate.query</to>
    </mapping>
    <mapping>
        <from>EJB_VIEWTYPE</from>
        <to>@andromda.ejb.viewType</to>
    </mapping>
    <mapping>
        <from>EJB_TRANSACTION_TYPE</from>
        <to>@andromda.ejb.transaction.type</to>
    </mapping>
    <mapping>
        <from>HIBERNATE_LAZY</from>
        <to>@andromda.hibernate.lazy</to>
    </mapping>
    <mapping>
        <from>HIBERNATE_INHERITANCE</from>
        <to>@andromda.hibernate.inheritance</to>
    </mapping>
    <mapping>
        <from>HIBERNATE_OUTER_JOIN</from>
        <to>@andromda.hibernate.outerjoin</to>
    </mapping>
    <mapping>
        <from>HIBERNATE_USE_QUERY_CACHE</from>
        <to>@andromda.hibernate.query.useCache</to>
    </mapping>
    <mapping>
        <from>HIBERNATE_ENTITY_CACHE</from>
        <to>@andromda.hibernate.entity.cache</to>
    </mapping>
    <mapping>
        <from>HIBERNATE_ENTITY_DYNAMIC_INSERT</from>
        <to>@andromda.hibernate.entity.dynamicInsert</to>
    </mapping>
    <mapping>
        <from>HIBERNATE_ENTITY_DYNAMIC_UPDATE</from>
        <to>@andromda.hibernate.entity.dynamicUpdate</to>
    </mapping>
    <mapping>
        <from>HIBERNATE_ASSOCIATION_CACHE</from>
        <to>@andromda.hibernate.association.cache</to>
    </mapping>
    <mapping>
        <from>HIBERNATE_EHCACHE_MAX_ELEMENTS</from>
        <to>@andromda.hibernate.ehcache.maxElementsInMemory</to>
    </mapping>
    <mapping>
        <from>HIBERNATE_EHCACHE_ETERNAL</from>
        <to>@andromda.hibernate.ehcache.eternal</to>
    </mapping>
    <mapping>
        <from>HIBERNATE_EHCACHE_TIME_TO_IDLE</from>
        <to>@andromda.hibernate.ehcache.timeToIdleSeconds</to>
    </mapping>
    <mapping>
        <from>HIBERNATE_EHCACHE_TIME_TO_LIVE</from>
        <to>@andromda.hibernate.ehcache.timeToLiveSeconds</to>
    </mapping>
    <mapping>
