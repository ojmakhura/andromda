    <!-- stereotypes -->
    <mapping>
        <from>FRONT_END_VIEW</from>
        <to>FrontEndView</to>
    </mapping>
    <mapping>
        <from>FRONT_END_EVENT</from>
        <to>FrontEndEvent</to>
    </mapping>
    <mapping>
