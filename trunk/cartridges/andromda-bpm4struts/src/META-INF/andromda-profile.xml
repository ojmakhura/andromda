    </mapping>
    <mapping>
        <from>FRONT_END_CONTROLLER</from>
        <to>FrontEndController</to>
    </mapping>
    <mapping>
        <from>FRONT_END_EXCEPTION</from>
        <to>FrontEndException</to>
    </mapping>
    <mapping>
        <from>FRONT_END_SESSION_OBJECT</from>
        <to>FrontEndSessionObject</to>
    </mapping>
    <!-- tagged values -->
