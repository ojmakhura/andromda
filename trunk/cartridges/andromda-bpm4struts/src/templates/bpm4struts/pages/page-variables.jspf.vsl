#set ($generatedFile = "${jsp.fullPath}-vars.jspf")
#if ($jsp.pageVariables.size() > 0)
<div id="pageVariables">
#foreach ($variable in $jsp.pageVariables)
#set ($formVariableName = "${formName}.${variable.name}")
    <c:catch>
#if ($setPageVariablesInPageScope=="true")
        <c:set var="$variable.name" value="${${formVariableName}}"/>
#end
#if (!$variable.table)
        <c:out value="${${formVariableName}}"/>
#end
    </c:catch>
#end
</div>
#end