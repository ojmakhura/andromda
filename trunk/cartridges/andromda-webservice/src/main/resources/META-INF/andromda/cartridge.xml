    <condition name="overwriteAll">$overwriteAll.equalsIgnoreCase("true")</condition>
    <condition name="xfire">$soapStack.equalsIgnoreCase("xfire")</condition>
    <condition name="axis">$soapStack.equalsIgnoreCase("axis")</condition>
    <condition name="axis1">#if ($soapStack.equalsIgnoreCase("axis") &amp;&amp; $axisVersion.equals("1"))true#end</condition>
    <condition name="axis2">#if ($soapStack.equalsIgnoreCase("axis") &amp;&amp; $axisVersion.equals("2"))true#end</condition>
    <condition name="jaxws">$soapStack.equalsIgnoreCase("jaxws")</condition>
    <condition name="cxf">$soapStack.equalsIgnoreCase("cxf")</condition>
    <condition name="xfireRef">#if ($soapStack.equalsIgnoreCase("xfire") &amp;&amp; $generateRefFiles.equals("true"))true#end</condition>
    <condition name="axisRef">#if ($soapStack.equalsIgnoreCase("axis") &amp;&amp; $generateRefFiles.equals("true"))true#end</condition>
