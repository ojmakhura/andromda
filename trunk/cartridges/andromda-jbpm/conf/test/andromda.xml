<andromda>
    <properties>
        <property name="cartridgeFilter">jbpm</property>
        <property name="failOnValidationErrors">false</property>
    </properties>
    <repositories>
        <repository name="netBeansMDR">
            <models>
                <model>
                    <uri>${andromda.cartridge.test.model.uri}</uri>
                    <moduleSearchLocations>
                        <location>${basedir}/../../etc/profile/src/uml</location>
                    </moduleSearchLocations>
                </model>
            </models>            
        </repository>
    </repositories>
    <namespaces>
        <namespace name="jbpm">
            <properties>
                <property name="languageMappingsUri">Java</property>
                <property name="wrapperMappingsUri">JavaWrapper</property>
                <property name="definitions">${test.output.dir}</property>
                <property name="nodes">${test.output.dir}</property>
                <property name="handlers">${test.output.dir}</property>
                <property name="handler-impls">${test.output.dir}</property>
                <property name="helpers">${test.output.dir}</property>
                <property name="clients">${test.output.dir}</property>
            </properties>
        </namespace>
    </namespaces>
</andromda>