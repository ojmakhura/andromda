<?xml version="1.0" encoding="ISO-8859-1" ?>

<!-- contains the hibernate cartridge metafacade mappings -->
<metafacades namespace="spring">
	<metafacade 
		metaobjectClass="org.omg.uml.foundation.core.Operation$Impl"
		metafacadeClass="org.andromda.cartridges.spring.metafacades.HibernateFinderMethodLogicImpl">
		<stereotype>FinderMethod</stereotype>
	</metafacade>	
	<metafacade 
		metaobjectClass="org.omg.uml.foundation.core.DataType$Impl"
		metafacadeClass="org.andromda.cartridges.spring.metafacades.HibernateTypeLogicImpl">
        <property reference="hibernateTypeMappingsUri" default=""/>
	</metafacade>
    <metafacade 
        metaobjectClass="org.omg.uml.foundation.core.UmlClass$Impl"
        metafacadeClass="org.andromda.cartridges.spring.metafacades.SpringServiceLogicImpl">
        <stereotype>Service</stereotype>
	    <property reference="ejbJndiNamePrefix" default=""/>
        <property reference="ejbPackageNamePattern" default="{0}.ejb"/>
    </metafacade>
    <metafacade 
        metaobjectClass="org.omg.uml.foundation.core.Interface$Impl"
        metafacadeClass="org.andromda.cartridges.spring.metafacades.SpringServiceLogicImpl">
        <stereotype>Service</stereotype>
        <property reference="ejbJndiNamePrefix" default=""/>
        <property reference="ejbPackageNamePattern" default="{0}.ejb"/>
    </metafacade>	
    <metafacade 
        metaobjectClass="org.omg.uml.foundation.core.UmlClass$Impl"
        metafacadeClass="org.andromda.cartridges.spring.metafacades.SpringEntityLogicImpl"
		contextRoot="true">
        <stereotype>Entity</stereotype>
		<property reference="entityNamePattern" default="{0}"/>
    </metafacade>
    <metafacade 
        metaobjectClass="org.omg.uml.foundation.core.Interface$Impl"
        metafacadeClass="org.andromda.cartridges.spring.metafacades.SpringEntityLogicImpl"
		contextRoot="true">
        <stereotype>Entity</stereotype>
		<property reference="entityNamePattern" default="{0}"/>
    </metafacade>
</metafacades>