<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>org.andromda.profiles.emf.rsm75</groupId>
        <artifactId>andromda-profiles-emf-rsm75</artifactId>
        <version>5.0-SNAPSHOT</version>
    </parent>
    <packaging>emx</packaging>
    <artifactId>andromda-datatype</artifactId>
    <name>AndroMDA RSM75 Datatype ModelLibrary</name>
    <description>
        Contains the AndroMDA RSM75 datatype profiles.
    </description>
    <scm>
      <connection>scm:svn:http://svn.code.sf.net/p/andromda/svn/trunk/profiles/emf/rsm75/datatype</connection>
      <developerConnection>scm:svn:svn+ssh://svn.code.sf.net/p/andromda/svn/trunk/profiles/emf/rsm75/datatype</developerConnection>
      <url>http://svn.code.sf.net/p/andromda/svn/trunk/profiles/emf/rsm75/datatype/</url>
    </scm>
    <build>
        <plugins>
            <plugin>
                <extensions>true</extensions>
                <groupId>org.andromda.maven.plugins</groupId>
                <artifactId>andromda-model-archiver-plugin</artifactId>
                <configuration>
                	<replaceExtensions>false</replaceExtensions>
                	<replacementExtensions>.emx</replacementExtensions>
                	<modelFilePattern>.*(\.emx)</modelFilePattern>
                </configuration>
            </plugin>
        </plugins>
    </build>
</project>
